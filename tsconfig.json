{
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "module": "commonjs",
    "outDir": "./out",
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "include": ["index.ts"],
  "exclude": ["node_modules", "out", "saves"]
}
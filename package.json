{
  "name": "playground",
  "version": "0.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "start": "node ./out/index.js",
    "start:dev": "ts-node index.ts",
    "dev": "nodemon --exec ts-node index.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "eslint-config-love": "54.0.0",
    "nodemon": "3.1.4",
    "prettier": "3.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.5.3"
  }
}
